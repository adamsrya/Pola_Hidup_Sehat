<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.polahidupsehat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PolaHidupSehat">
        <activity android:name=".riwayatkons"></activity>
        <activity android:name=".reminder" />
        <activity android:name=".anjuran" />
        <activity android:name=".polaolga" />
        <activity android:name=".polamakan" />
        <activity
            android:name=".konsultasi"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".hasilkonsul"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".replacemenufr" />
        <activity android:name=".daftar" />
        <activity android:name=".login" />
        <activity
            android:name=".menu"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.ASSIST" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>